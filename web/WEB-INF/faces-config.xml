<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <resource-bundle>
            <base-name>com.flowyk.fb.i18n.Bundle</base-name>
            <var>Loc</var>
        </resource-bundle>
        <locale-config>
            <default-locale>sk</default-locale>
        </locale-config>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
    </application>
    <navigation-rule>
        <navigation-case>
            <from-outcome>presslike</from-outcome>
            <to-view-id>/contest/presslike.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>register</from-outcome>
            <to-view-id>/contest/register.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>thanks</from-outcome>
            <to-view-id>/contest/thanks.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>unactive</from-outcome>
            <to-view-id>/contest/page-unactive.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>returning</from-outcome>
            <to-view-id>/contest/returning.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>contest-rules</from-outcome>
            <to-view-id>/contest-rules.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>admin</from-outcome>
            <to-view-id>/contest/pageAdmin/admin.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>user-list</from-outcome>
            <to-view-id>/contest/contestAdmin/user-list.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>contest-edit</from-outcome>
            <to-view-id>/contest/contestAdmin/contest-edit.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>admin-page-unactive</from-outcome>
            <to-view-id>/contest/pageAdmin/admin-page-unactive.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
